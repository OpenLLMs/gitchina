<%- include('./_partial/header', { tabCls: 'users', type: 'global', title: 'GitChina-Github全球中国开发者、仓库、机构、项目最热门排行版榜单' }); %>
<div class="userlist">
  <%- include('./_partial/users_nav', { type: 'global' }); %>
<table>
  <thead>
    <tr>
      <th title="avatar">ava</th>
      <th>login</th>
      <th>location</th>
      <th>company</th>
      <th data-sort="followers">followers</th>
      <th data-sort="following">following</th>
      <th>repos</th>
      <th>created at</th>
    </tr>
  </thead>
  <tbody>
    <% USERS.forEach(function(user, idx) {%>
    <%- include('./_partial/users_list', { user, idx }); %>
    <% }); %>
  </tbody>
</table>
</div>
<%- include('./_partial/footer', { }); %>